{
  "name": "kaartdijin_boodja",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-mock": "vite --mode mock",
    "build": "vue-tsc --noEmit && vite build",
    "build:watch": "vue-tsc --noEmit --watch && vite build",
    "preview": "npm run build && vite preview",
    "test": "vitest",
    "lint": "esw --ext .ts,.vue --ignore-path .gitignore src",
    "lint:watch": "esw --ext .ts,.vue --ignore-path .gitignore --watch src",
    "format": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.2",
    "luxon": "^3.0.4",
    "pinia": "^2.0.22",
    "vue": "^3.2.37",
    "ol": "^7.2.2",
    "colormap": "^2.3.2",
    "kompas": "^0.0.1",
    "ol-mapbox-style": "^9.0.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.1.0",
    "autoprefixer": "10.4.12",
    "eslint": "^8.24.0",
    "eslint-watch": "^8.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.6.0",
    "happy-dom": "^6.0.4",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "sass": "^1.55.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vitest": "^0.23.4",
    "vue-router": "^4.1.5",
    "vue-tsc": "^0.40.4",
    "eslint-config-openlayers": "^16.2.3",
    "vite-plugin-static-copy": "^0.7.0"
  }
}
